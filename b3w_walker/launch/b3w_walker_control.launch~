<launch>
  <rosparam file="$(find b3w_walker)/config/b3w_walker_control.yaml" command="load"/>

  <node name="controller_spawner" pkg="controller_manager" type="spawner" respawn="false" output="screen" ns="/b3w_walker" args="joint_group_controller joint_state_controller"/>

  <!-- convert joint states to TF transforms for rviz, etc -->
  <node name="robot_state_publisher" pkg="robot_state_publisher" type="robot_state_publisher"
  	respawn="false" output="screen">
    <remap from="/joint_states" to="/b3w_walker/joint_states" />
  </node>

</launch>
